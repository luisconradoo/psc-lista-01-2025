programa
{
  
funcao inicio()
  {
   
    cadeia nome
    inteiro idade 
    real altura 

    escreva("Digite seu nome completo: ")
    leia(nome)

    escreva("Digite sua idade: ")
    leia(idade)

    escreva("Digite sua altura (em centímetros): ")
    leia(altura)

    escreva("\nDados do usuário:\n")
    escreva("Nome: ", nome, "\n")
    escreva("Idade: ", idade, " anos\n")
    escreva("Altura: ", altura, " centímetros\n")

  }
}
